{
    "problem": "GenericTensor",
    "tensor_formulation": "NeoHookean",

    "nl_solver": "newton",
    "solver_params": {
        "mtype": 2,
        "gradNorm": 1e-5,
        "useGradNorm": true
    },
    "solver_type": "Eigen::PardisoLDLT",
    "line_search": "backtracking",
    "lump_mass_matrix": false,

    "has_collision": true,
    "dhat": 1e-3,

    "normalize_mesh": false,

    "export": {
        "paraview": "sol.vtu",
        "#wireframe": true,
        "body_ids": true,
        "material_params": true,
        "high_order_mesh": false
    },
    "save_time_sequence": true,
    "save_solve_sequence_debug": false,
    "vismesh_rel_area": 1e5,
    "output": "sim.json",

    "discr_order": 1,
    "problem_params": {
        "is_time_dependent": true,
        "dirichlet_boundary": [{
            "id": 1,
            "value": [0, 0]
        }, {
            "id": 2,
            "value": [0, "((-0.1 + 0.99)*(x-0.5) - 0.1) * t"]
        }],
        "rhs": [0, 0, 0]
    },

    "tend": 1,
    "dt": 0.1,
    
    "boundary_sidesets": [{
        "id": 1,
        "axis": "-y",
        "position": 0,
        "relative": true
    }, {
        "id": 2,
        "axis": "y",
        "position": 1,
        "relative": true
    }],
    
    "body_params": [{
        "id": 1,
        "E": 6e5,
        "nu": 0.4,
        "rho": 1000
    }]
}
