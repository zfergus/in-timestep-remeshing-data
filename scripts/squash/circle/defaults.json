{
    "problem": "GenericTensor",
    "tensor_formulation": "NeoHookean",

    "nl_solver": "newton",
    "solver_params": {
        "mtype": 2,
        "gradNorm": 1e-5,
        "useGradNorm": true
    },
    "solver_type": "Eigen::PardisoLDLT",
    "line_search": "backtracking",
    "lump_mass_matrix": false,

    "has_collision": true,
    "dhat": 1e-3,

    "normalize_mesh": false,

    "export": {
        "paraview": "sol.vtu",
        "#wireframe": true,
        "body_ids": true,
        "material_params": true,
        "high_order_mesh": false
    },
    "save_time_sequence": true,
    "save_solve_sequence_debug": false,
    "vismesh_rel_area": 1e5,
    "output": "sim.json",

    "discr_order": 1,
    "problem_params": {
        "is_time_dependent": true,
        "rhs": [0, 0, 0]
    },

    "tend": 2,
    "dt": 0.1,
    "mu": 1,
    
    "body_params": [{
        "id": 1,
        "E": 1e5,
        "nu": 0.4,
        "rho": 1000
    }],

    "obstacles":[{
        "mesh": "../../../meshes/squircle.msh",
        "position": [0, 0.76],
        "dimensions": [0.5, 0.5],
        "displacement": [0, "if(t - 1, 5, -t)", 0]
    }, {
        "mesh": "../../../meshes/obstacles/line.obj",
        "position": [0, -0.51, 0]
    }]
}
